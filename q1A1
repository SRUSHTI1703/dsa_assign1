package DSA;
import java.util.Scanner;
public class Maindsa {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int count=0;
        while(n>2){
            n=n/2;
            count++;


        } System.out.println(count);

        }
    }
